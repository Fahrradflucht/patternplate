{
  "name": "@patternplate/cli",
  "version": "1.0.0",
  "description": "Create, show and deliver component libraries",
  "bin": {
    "patternplate": "./lib/cli.js"
  },
  "scripts": {
    "babel": "babel src --out-dir ./lib",
    "build": "npx yarn run babel",
    "clean": "npx rimraf lib",
    "watch": "npm run babel -- --watch"
  },
  "xo": false,
  "babel": {
    "presets": [
      "es2015-node4",
      "es2016-node4",
      "es2017"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "dependencies": {
    "@patternplate/client": "^1.0.0",
    "app-root-path": "^1.0.0",
    "babel-polyfill": "^6.5.0",
    "boxen": "^0.8.0",
    "chalk": "^2.3.0",
    "cp-file": "^3.2.0",
    "default-shell": "^1.0.1",
    "express": "^4.16.2",
    "express-slash": "^2.0.1",
    "find-root": "^0.1.1",
    "globby": "^6.0.0",
    "import-from": "^2.1.0",
    "is-ci": "^1.0.10",
    "is-stream": "^1.1.0",
    "listr": "^0.7.0",
    "lodash": "^4.14.0",
    "meow": "^3.7.0",
    "minimist": "^1.1.1",
    "normalize-package-data": "^2.3.5",
    "normalize.less": "^1.0.0",
    "opn": "^4.0.2",
    "ora": "^0.3.0",
    "path-exists": "^3.0.0",
    "resolve": "^1.1.7",
    "resolve-path": "^1.2.2",
    "sander": "^0.6.0",
    "stream-to-string": "^1.1.0",
    "throat": "^3.0.0",
    "zen-observable": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.0.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-es2016-node4": "^6.0.1",
    "babel-preset-es2017": "^6.14.0",
    "errorhandler": "^1.5.0",
    "rimraf": "^2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "sinnerschrader/patternplate"
  },
  "author": {
    "name": "Mario Nebl",
    "email": "mario.nebl@sinnerschrader.com"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 4",
    "npm": ">= 3"
  }
}
