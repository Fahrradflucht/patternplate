{
  "name": "patternplate-transform-react-to-markup",
  "version": "1.1.0",
  "description": "patternplate transform creating markup from react components",
  "main": "distribution/index.js",
  "files": [
    "distribution"
  ],
  "scripts": {
    "start": "npm run watch",
    "clean": "rm -rf distribution",
    "build": "babel source -d distribution",
    "pretest": "npm run build && npm run deps",
    "test": "ava",
    "deps": "dependency-check . --missing && dependency-check . --extra --no-dev",
    "watch": "babel source -d distribution -w"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015-node4",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sinnerschrader/patternplate-transform-react-to-markup.git"
  },
  "bugs": {
    "url": "https://github.com/sinnerschrader/patternplate-transform-react-to-markup/issues"
  },
  "homepage": "https://github.com/sinnerschrader/patternplate-transform-react-to-markup#readme",
  "keywords": [
    "react",
    "patternplate",
    "transform",
    "pattern"
  ],
  "author": {
    "name": "SinnerSchrader Deutschland GmbH",
    "email": "jobs@sinnerschrader.com"
  },
  "contributors": [
    {
      "name": "Mario Nebl",
      "email": "mario.nebl@sinnerschrader.com"
    },
    {
      "name": "Markus Wolf",
      "email": "markus.wolf@sinnerschrader.com"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "ava": "0.18.2",
    "babel-cli": "6.18.0",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015-node4": "2.1.1",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.18.0",
    "dependency-check": "2.6.0",
    "parallelshell": "2.0.0",
    "react": "15.4.2",
    "react-dom": "15.4.1",
    "unexpected": "10.26.3"
  },
  "peerDependencies": {
    "react": ">= 0.14",
    "react-dom": ">= 0.14"
  },
  "dependencies": {
    "chalk": "1.1.3",
    "compare-semver": "1.1.0",
    "lodash": "4.17.2",
    "resolve": "1.3.2",
    "try-require": "1.2.1"
  }
}
