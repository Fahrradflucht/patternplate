  "dependencies": {
    "color": "2.0.0",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-side-effect": "1.0.2",
    "styled-components": "2.1.2",
    "tag-hoc": "1.0.0"
