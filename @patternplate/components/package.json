    },
    {
      "name": "Mario Nebl",
      "email": "mario.nebl@sinnerschrader.com"
    }
  ],
  "license": "MIT",
  "engines": {
    "node": ">= 4",
    "npm": ">= 3"
  },
  "babel": {
    "ignore": [
      "demo.js"
    ],
